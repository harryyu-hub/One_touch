{
    "manifest_version": 2,
    "name": "Package Tracker",
    "version": "1.0",

    "browser_action": {
        "default_icon": "icon.png",
        "default_title": "PackageTracker",
        "default_popup": "popup.html" // optional
    },

    "permissions": [
      "storage",
      "tabs"
    ]
}